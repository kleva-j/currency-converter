    "babel-preset-env": "^1.7.0",
    "browserify": "^16.5.2",
    "express": "^4.17.1",
    "idb": "^2.1.3",
    "nodemon": "^2.0.6",
    "snyk": "^1.413.3"
  },
  "snyk": true
}
